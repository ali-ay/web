.helpForMyWork
  width 100%
  max-width 770px
  margin 0px auto
  padding 40px 0 0 0

  .title
    font-size 18px
    font-weight Bold
    letter-spacing 2.6px
    color #202020
    padding-bottom 10px

.dropdownMenu
  .dropdown
    .dropdown-menu

      .dropdown-item
        font-size 16px
        border-bottom 1px solid #dfdfdf
        padding 16px 0
        letter-spacing normal

        &:last-child
          border-bottom none

        &:hover
          background-color #fff
